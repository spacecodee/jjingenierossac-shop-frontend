{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "ES2022",
    "module": "preserve",
    "paths": {
      "@app/*": [
        "src/app/*"
      ],
      "@core/*": [
        "src/app/core/*"
      ],
      "@shared/*": [
        "src/app/shared/*"
      ],
      "@layouts/*": [
        "src/app/layouts/*"
      ],
      "@features/*": [
        "src/app/features/*"
      ],
      "@environments/*": [
        "src/environments/*"
      ],
      "@spartan-ng/helm/avatar": [
        "./libs/ui/ui-avatar-helm/src/index.ts"
      ],
      "@spartan-ng/helm/button": [
        "./libs/ui/ui-button-helm/src/index.ts"
      ],
      "@spartan-ng/helm/card": [
        "./libs/ui/ui-card-helm/src/index.ts"
      ],
      "@spartan-ng/helm/form-field": [
        "./libs/ui/ui-form-field-helm/src/index.ts"
      ],
      "@spartan-ng/helm/icon": [
        "./libs/ui/ui-icon-helm/src/index.ts"
      ],
      "@spartan-ng/helm/input": [
        "./libs/ui/ui-input-helm/src/index.ts"
      ],
      "@spartan-ng/helm/label": [
        "./libs/ui/ui-label-helm/src/index.ts"
      ],
      "@spartan-ng/helm/progress": [
        "./libs/ui/ui-progress-helm/src/index.ts"
      ],
      "@spartan-ng/helm/sidebar": [
        "./libs/ui/ui-sidebar-helm/src/index.ts"
      ],
      "@spartan-ng/helm/tooltip": [
        "./libs/ui/ui-tooltip-helm/src/index.ts"
      ],
      "@spartan-ng/helm/typography": [
        "./libs/ui/ui-typography-helm/src/index.ts"
      ],
      "@spartan-ng/helm/utils": [
        "./libs/ui/ui-utils-helm/src/index.ts"
      ],
      "@spartan-ng/helm/separator": [
        "./libs/ui/ui-separator-helm/src/index.ts"
      ],
      "@spartan-ng/helm/sheet": [
        "./libs/ui/ui-sheet-helm/src/index.ts"
      ],
      "@spartan-ng/helm/aspect-ratio": [
        "./libs/ui/ui-aspect-ratio-helm/src/index.ts"
      ],
      "@spartan-ng/helm/skeleton": [
        "./libs/ui/ui-skeleton-helm/src/index.ts"
      ]
    }
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "typeCheckHostBindings": true,
    "strictTemplates": true
  },
  "files": [],
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.spec.json"
    }
  ]
}
